{
  "name": "typescript-node",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node ./dist/main.js",
    "dev": "nodemon --exec node --no-warnings --loader ts-node/esm src/main.ts",
    "build": "tsc",
    "prisma": "npx prisma migrate dev --name init && npx prisma generate",
    "postinstall": "npx prisma generate"
  },
  "prisma": {
    "seed": "node --loader ts-node/esm prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.16.1"
  },
  "devDependencies": {
    "dotenv": "16.4.5",
    "nodemon": "^3.1.4",
    "@types/node": "^20.14.9",
    "typescript": "^5.5.3",
    "ts-node": "^10.9.2",
    "prisma": "^5.16.1"
  }
}